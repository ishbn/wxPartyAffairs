.title{
  width: 75%;
  height: 60rpx;
  border-left: 2px solid red;
}
.inner{
  width: 95%;
  height: 100%;
  position: relative;
  left: 2.5%;
  line-height: 60rpx;
}
.text-all{
  width: 17%;
  height: 60rpx;
  color: darkgray;
  font-size: 15px;
  line-height: 60rpx;
  text-align: center;
}
.text-all::after{
    content: " >";
    /* color: blue; */
}
.content{
  width:100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
}
.item{ 
  display: flex;
  flex-wrap: wrap;
  width: 100%;
  height: 350rpx;
}
.lastest-class{
  width: 750rpx;
  display: flex;
  flex-wrap: wrap;
}
.lastest-class_images-img{
  position: relative;
  left: 20rpx;
  width:335rpx;
  height:220rpx;
}

.lastest-class_text{
  position: relative;
  left: 20rpx;
}

.list_item_text {
  height: 80rpx;
  line-height: 40rpx;
  font-size: 30rpx;
  width: 335rpx;
  overflow: hidden;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;
  text-overflow: ellipsis;
}
.look{
  width: 50rpx;
  height: 40rpx;
}
.list_item_updateTime {
  font-size: 30rpx;
  color:#bfbfbf;
}
.more{
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 30rpx;
  background: #EDECE8;
  color: darkgray;
  text-align: center;
  z-index: 0; 
}
/* 记录为空时 */
.null{
  width: 750rpx;
  height: 750rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.null-img{
  width: 120rpx;
  height: 110rpx;
}
.null-text{
  font-size: 30rpx;
  color: #e6e6e6;
}
