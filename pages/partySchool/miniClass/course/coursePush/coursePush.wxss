swiper{
  width: 100%;
  height: 350rpx;
  background: -webkit-linear-gradient(top, #ff8f5b , white);
}
swiper-item{
  padding-top: 25rpx;
}
image{
  width: 600rpx;
  height: 300rpx;
  position: absolute;
}
image.active{
  transform: scale(1.2);
  transition:all .2s ease-in 0s;
}
.head{
  width: 100%;
  height:100rpx;
  color:white;
  text-align:center;
  line-height: 100rpx;
  background: -webkit-linear-gradient(top, #dc4824 , #ff8f5b);
}
.swp-info{
  width: 570rpx;
  height: 150rpx;
  display: flex;
  flex-direction: column;
}
.swp-title {
  width: 100%;
  height: 40%;
  line-height: 60rpx;
  font-weight: bold;
  text-align: center;
  font-size: 35rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.progress{
  width: 100%;
  height: 30%;
}
.under{
  width: 100%;
  height: 30%;
  display: flex;
}
.swp-end {
  width: 50%;
  height: 100%;
  line-height: 45rpx;
  color: darkgray;
  font-size: 30rpx;
  display: flex;
  justify-content: flex-end;
}
.swp-rest {
  width: 50%;
  height: 100%;
  line-height: 45rpx;
  color: darkgray;
  font-size: 30rpx;
}
.main {
  height: 600rpx;
  width: 750rpx;
  border-bottom: 10px solid #EDECE8;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
}
