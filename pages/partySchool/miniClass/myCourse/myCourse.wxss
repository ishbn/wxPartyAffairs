.main{
  display: flex;
  flex-wrap: wrap;
}
.head{
  width: 750rpx;
  height: 85rpx;
  display: flex;
  justify-content:center;
  align-items: center;
  border-bottom: thin solid darkgray;  
  background: white;
}
.menu{
  width: 90%;
  height: 60%;
  background: white;
  display: flex;
}
.menu-item{
  color:#dc5739;
  border: 1rpx solid #dc5739;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 32rpx;
  width: 50%;
  height: 100%;
}
.menu-item.active{
  background: #dc5739;
  color: white;
}
.view{
  width:100%;
  height:80rpx;
  border: thin solid white;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #eeeeee;
}
.header{
  width:95%;
  height:70rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.learn{
  width: 90%;
  height: 100%;
  color: darkgray;
  font-size: 30rpx;
  line-height: 70rpx;
}
.learning_up{
  width:40rpx;
  height: 40rpx;
  transform: rotate(180deg);
}
.learning_down{
  width:40rpx;
  height: 40rpx;
}
.learned_up{
  width:40rpx;
  height: 40rpx;
  transform: rotate(180deg);
}
.learned_down{
  width:40rpx;
  height: 40rpx;
}
#box{
  overflow: hidden;
  animation-fill-mode: forwards;
}
.show{
  width: 750rpx;
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
}
.oneVedio{
  width: 94%;
  height: 150rpx;
  display: flex;
  align-items: center;
}
.vedioImg{
  width: 20%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.img{
  width: 130rpx;
  height: 100rpx;
}
.message{
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 80%;
  height: 80%;
}
.title{
  width: 100%;
  height: 50rpx;
  font-size: 30rpx;
  line-height: 50rpx;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
.Text{
  width: 100%;
  height: 40rpx;
  font-size: 25rpx;
  line-height: 40rpx;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
  color: darkgray;
}
.update{
  width: 100%;
  height: 30rpx;
  display: flex;
  align-items: center;
}
.updatetime{
  font-size: 25rpx;
  color: darkgray;
  display: flex;
  align-items: center;
  line-height: 30rpx;
}
.doc{
  width: 100%;
  height: 100%;
}
.must{
  /* position: relative; */
  left: 3%;
  width: 100%;
  height: 150rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom-style: solid;
  border-bottom-color: #eeeeee;
  border-bottom-width: 3rpx;
}
navigator{
  width: 100%;
  height: 100%;
}
.classImg{
  width: 20%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  margin:0 10px;
}
.img{
  width: 100%;
  height: 100%;
}
.message{
  width: 75%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.title{
  width: 100%;
  height: 45rpx;
  display: flex;
  align-items: center;
}
.title-text{
  font-size: 32rpx;
  width:85%;
  height: 100%;
  line-height: 50rpx;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
.title-img{
  width: 70rpx;
  height: 30rpx;
}
.intro{
  width: 100%;
  height: 45rpx;
  font-size: 30rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  color: darkgray;
}
.labelList{
  width: 100%;
  height: 30rpx;
  display: flex;
}
.lab{
  font-size: 25rpx;
  line-height: 30rpx;
  color: rgb(125, 125, 250);
  margin-right: 10rpx;
}
.did{
  width: 750rpx;
  height: 750rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.null-img{
  width: 120rpx;
  height: 110rpx;
}
.null-text{
  font-size: 30rpx;
  color: #e6e6e6;
}
.noMore{
  width:750rpx;
  height: 0;
  line-height:150rpx;
  font-size: 30rpx;
  color: darkgray;
  text-align: center;
}
.more{
  height: 50rpx;
  line-height: 50rpx;
  font-size: 30rpx;
  color: darkgray;
  text-align: center;
  background: #eeeeee;
}
/* 记录为空时 */
.null{
  width: 750rpx;
  height: 750rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.null-img{
  width: 120rpx;
  height: 110rpx;
}
.null-text{
  font-size: 30rpx;
  color: #e6e6e6;
}
