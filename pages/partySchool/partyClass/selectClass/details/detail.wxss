.main{
  display: flex;
  flex-wrap: wrap;
}
.head{
  width: 750rpx;
  height: 80rpx;
  border-bottom: thin solid #e6e6e6;
  border-top: thin solid #e6e6e6;
  display: flex;
  justify-content: center;
  align-items: center;
}
.inner{
  width: 200rpx;
  height: 70rpx;
  font-size: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.arrow{
  width: 40rpx;
  height: 40rpx;
}
@keyframes display_show{
  from {
    height: 0rpx;
  }

  to {
    height: 650rpx;
  }
}
@keyframes display_none{
  from {
    height: 650rpx;
  }

  to {
    height: 0rpx;
  }
}
.display_up{
  transform: rotate(180deg);
}
.display_show{
  animation: display_show 1s;
}
.display_none{
  animation: display_none 1s;
}
.show{
  width: 750rpx;
  height: 650rpx;
  position: absolute;
  top: 80rpx;
  z-index: 3;
  display: flex;
}
.hide{
  display: none;
}
#box{
  width: 100%;
  border-top: 1px solid #ddd;
  overflow: hidden;
  height: 0;
  animation-fill-mode: forwards;
}
.shadow{
  width: 750rpx;
  background: rgba(1, 1, 1, 0.2);
  position: absolute;
  z-index: 2;
  top: 80rpx;
}
.type{
  width: 30%;
  height: 100%;
  background: #e6e6e6;
}
.out{
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
}
.in{
  width: 100%;
  height: 70rpx;
  font-size: 32rpx;
  text-align: center;
  line-height: 70rpx;
}
.change{
  color: red;
  border-left: 5rpx solid red;
  background: white;
  font-size: 35rpx;
}
.label{
  width: 70%;
  height: 100%;
  background: white;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.label-item{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 32rpx;
}
.contain{
  width: 750rpx;
  display: flex;
  flex-wrap: wrap;
}
.all{
  width: 750rpx;
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
}
.oneVedio{
  width: 94%;
  height: 150rpx;
  display: flex;
  align-items: center;
  border-bottom: thin solid #e6e6e6;
}
.vedioImg{
  width: 20%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.img{
  width: 130rpx;
  height: 100rpx;
}
.message{
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 80%;
  height: 80%;
}
.title{
  width: 100%;
  height: 50rpx;
  font-size: 30rpx;
  line-height: 50rpx;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
.Text{
  width: 100%;
  height: 40rpx;
  font-size: 25rpx;
  line-height: 40rpx;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
  color: darkgray;
}
.update{
  width: 100%;
  height: 30rpx;
  display: flex;
  align-items: center;
}
.updatetime{
  font-size: 25rpx;
  color: darkgray;
  display: flex;
  align-items: center;
  line-height: 30rpx;
}
.more{
  height: 50rpx;
  width: 750rpx;
  line-height: 50rpx;
  font-size: 30rpx;
  color: darkgray;
  text-align: center;
  background: #eeeeee;
}
/* 记录为空时 */
.null{
  width: 750rpx;
  height: 750rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.null-img{
  width: 120rpx;
  height: 110rpx;
}
.null-text{
  font-size: 30rpx;
  color: #e6e6e6;
}
