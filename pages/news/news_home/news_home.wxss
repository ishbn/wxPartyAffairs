.index {
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
}

.headimg {
  height: 420rpx;
  width: 100%;
}

.classification {
  display: flex;
  flex-wrap: wrap;
}

navigator {
  width: 50%;
  height: 100%;
}

.box {
  width: 100%;
  justify-content: center;
  border: 2rpx solid rgb(238, 238, 238);
}

.menu_list {
  width: 100%;
  height: 100%;
  margin-top: 40rpx;
  margin-bottom: 30rpx;
}

.menu_img {
  width: 64rpx;
  height: 64rpx;
  display: block;
  margin: 0 auto;
}

.menu_name {
  display: block;
  width: 100%;
  text-align: center;
  padding-top: 15rpx;
  font-size: 32rpx;
}

.description {
  display: block;
  width: 100%;
  text-align: center;
  font-size: 24rpx;
  padding-top: 15rpx;
  color: gray;
}
