<!--pages/wode/personalInfo/menu/menu.wxml-->
<view class='wholePage'>
  <view class='header'>
    <image src='https://cn.bing.com/th?id=OSA.E25992E925DA7D7BD3DA068D81B79E21&pid=MSNJVFeeds'></image>
  </view>
  <view class='menuList'>
    <block wx:for='{{menu}}' wx:key="{{index}}" >
        <view class='menu' bindtap='navigateTo' data-index='{{index}}'>

            <image  src='{{item.icon}}'></image>
            <text>{{item.name}}</text>
        </view>

    </block>
    
  </view>
</view>
