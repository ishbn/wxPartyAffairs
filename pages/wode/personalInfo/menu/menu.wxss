/* pages/wode/personalInfo/menu/menu.wxss */
.wholePage{
  background: rgb(241, 237, 237);

}
.header{
  width: 100%;
  height: 420rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 30rpx;
}
.header image{
  width: 100%;
  height: 100%;
}
.menuList{
  width: 100%;
  background: #fff;
}
.menu{
  width:90%;
  height: 100rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  border-bottom: 1rpx solid rgb(241, 237, 237);
  margin: 10rpx 40rpx;
}
.menu image{
  width: 32rpx;
  height: 32rpx;
  margin-right: 25rpx;
}
.menu text{
  font-size: 32rpx;
  color:darkcyan;
  /* margin: 0 15rpx; */
}
