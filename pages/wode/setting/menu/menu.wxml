<!--pages/wode/personalInfo/menu/menu.wxml-->
<view class='wholePage'>
  <view class='header'>
    <image src='{{bgImg}}'></image>
  </view>
  <view class='menuList'>
    <block wx:for='{{menu}}' wx:key="{{index}}" >
        <view class='menu' bindtap='navigateTo' data-index='{{index}}'>

            <image  src='{{item.icon}}'></image>
            <text>{{item.name}}</text>
        </view>

    </block>
    
  </view>
</view>
