/* pages/wode/feedback/mysubmit_list/mysubmit_list.wxss */
.feedback_list_item{
  margin: 10rpx 10rpx;
}


.list_item{
    display: block;
    padding: 15px 15px 10px 10px;
    overflow: hidden;
    cursor: default;
    text-decoration: none;
    -webkit-tap-highlight-color: transparent;
    border-bottom:2rpx solid rgb(238, 238, 238);
}


.list_item_title{
  width: 100%;
  height: 60%; 
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  font-size: 35rpx;


    overflow: hidden;
    -ms-text-overflow: ellipsis;
    text-overflow: ellipsis; 
    display: -webkit-box;
    -webkit-box-orient: vertical; /** 设置或检索伸缩盒对象的子元素的排列方式 **/
    -webkit-line-clamp: 2; /** 显示的行数 **/
}

.list_item_text{
  overflow: hidden;
  display: flex;
}

.list_item_type{
     font-weight: normal;
    font-size: 32rpx;
    color: #f44236;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-wrap: normal;
    margin-top:20rpx; 
}
.list_item_date{
  font-size: 13px;
  color: #999;
  margin: 20rpx 20rpx;
  display: flex;
  align-items: center;
}
.tail{
  font-size: 24rpx;
  color: grey;
  text-align: center;
  padding-top: 10rpx;
  padding-bottom: 10rpx; 
  border-bottom: 2rpx solid rgb(238, 238, 238);
}